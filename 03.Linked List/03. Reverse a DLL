/*
class ListNode {
    int data;
    ListNode prev, next;
    ListNode(int val) {
        this.data = val;
        this.prev = null;
        this.next = null;
    }
}
*/

class Solution {
    public ListNode reverseDLL(ListNode head) {
        if(head ==null || head.next== null) return head;
        
        ListNode prev = null;
        ListNode current = head;
        while(current!= null){
            prev = current.prev;

            current.prev= current.next;
            current.next= prev;

            current= current.prev;
        }
        return prev.prev;
    }
}
