class Solution {
    public int[] findMissingRepeatingNumbers(int[] nums) {
       int n = nums.length;
       long s21=(long)n*(n+1)/2;
       long s22= (long)n*(n+1)*(2*n+1)/6;

       long s1=0, s2=0;

       for(int num : nums){
        s1+=num;
        s2 += (long)num *num;
       }

       long val1= s1-s21;
       long val2= s2-s22;
       val2= val2/val1;

       long x= (val1+val2)/2;
       long y = x-val1;

       return new int[]{(int)x, (int) y};

    }
}
