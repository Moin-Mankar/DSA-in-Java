class Solution {
    public List<Integer> majorityElement(int[] nums) {
        int count1=0,count2=0;
        int el1= Integer.MIN_VALUE;
        int el2= Integer.MIN_VALUE;

        for(int i=0;i<nums.length;i++){
            if(count1==0 && count2!=nums[i]){
                count1=1;
                el1=nums[i];
            }else if(count2==0 && count1!=nums[i]){
                count2=1;
                el2= nums[i];
            }else if(nums[i]==el1){
                count1++;
            }else if(nums[i]==el2){
                count2++;
            }else{
                count1--;
                count2--;
            }
        }
        count1=0;
        count2=0;

        for(int i =0;i<nums.length;i++){
            if(el1==nums[i]){
                count1++;
            }
            if(el2==nums[i]){
                count2++;
            }
        }
        int count = nums.length/3;

        List<Integer> ans = new ArrayList<>();

        if(count1>count){
            ans.add(el1);
        }
        if(count2>count){
            ans.add(el2);
        }
        Collections.sort(ans);

        return ans;
    }
}
