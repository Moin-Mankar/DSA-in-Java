class Solution {
    public int maxProfit(int[] prices) {
        int mini = prices[0];
        int maxProf= 0;

        for(int i=1; i< prices.length;i++){
            int price = prices[i]- mini;
            maxProf = Math.max(maxProf, price);
            mini = Math.min(mini, prices[i]);
        }
        return maxProf;
    }
}
